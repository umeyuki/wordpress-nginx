server {
  listen 80;
  server_name sandbox.umeyuki.net;
  access_log /var/log/nginx/sandbox.umeyuki.net/access.log;
  error_log  /var/log/nginx/sandbox.umeyuki.net/error.log;                                                            
  root /var/www/sandbox.umeyuki.net;
  location / {
      index index.html;
 }
}

